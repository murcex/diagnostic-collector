<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <!-- Add this to the host application -->
  <configSections>
    <section name="Kiroku" type="System.Configuration.AppSettingsSection" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <!--KFlowSettings-->
  <appSettings>
    <!--Main-->
    <add key="instanceloop" value="1" />
    <add key="blockloop" value="1" />
    <!--Trace-->
    <add key="trace" value="1" />
    <add key="traceloop" value="1" />
    <add key="tracechar" value="1" />
    <!--Info-->
    <add key="info" value="1" />
    <add key="infoloop" value="1" />
    <add key="infochar" value="1" />
    <!--Warning-->
    <add key="warning" value="1" />
    <add key="warningloop" value="1" />
    <add key="warningchar" value="1" />
    <!--Error-->
    <add key="error" value="1" />
    <add key="errorloop" value="1" />
    <add key="errorchar" value="1" />
  </appSettings>
  <Kiroku>
    <!--WriterSwitch-->
    <add key="write" value="0" />
    <!--GUID-->
    <add key="applicationid" value="00000000-0000-0000-0000-000000000000" />
    <add key="trackid" value="00000000-0000-0000-0000-000000000000" />
    <!--EventSwitch-->
    <add key="verbose" value="0" />
    <add key="trace" value="0" />
    <add key="info" value="0" />
    <add key="warning" value="0" />
    <add key="error" value="0" />
    <!--FilePath-->
    <add key="filepath" value="C:\\Temp\\Log\\MyApp\\" />
  </Kiroku>
</configuration>